<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hermes.app.persistence.pay.user.PayaccountsecInfoMapper">
    <select id="selectUserMd5key" parameterType="String"
            resultType="String">
           select md5key from TB_PayAccountSecInfo where usercode =#{usercode} and datastatus=1
    </select>
</mapper>

